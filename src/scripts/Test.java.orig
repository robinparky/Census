import java.io.File;
import java.util.*;

public class Test
{
    public static void main(String args[]) throws Exception
    {

	int hw = Integer.parseInt(args[0]);
	int i = Integer.parseInt(args[1]);
	int j = Integer.parseInt(args[2]);
    
	int X;
	int Y;
	
	if(hw>=j)
	{
	    X=i;
	    Y=(i+j)-hw;
	}
	else
	{
	    //X=2*hw-1-j;
	    X=i-j+hw;
	    Y=i;
	}

	System.out.println(X + " " + Y);


    }
}

